<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
  <class name="Anuncio" table="Anuncio">
    <id name="Id" access="property" column="idAnuncio">
      <generator class="native" />
    </id>
    <property name="Titulo" column="titulo" />
    <property name="Descripcion" column="descripcion" />
    <property name="FechaPublicacion" column="fechaPublicacion" />
    <property name="Estado" column="estado" />
    <set name="Aptitudes" table="aptitud_anuncio"
          inverse="false" lazy="true" fetch="select" cascade="all">
      <key column="idAnuncio" />
      <many-to-many column="idAptitud" class="Aptitud" />
    </set>
  </class>
</hibernate-mapping>