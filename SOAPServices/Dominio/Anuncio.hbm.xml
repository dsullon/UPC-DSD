<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio">
  <class name="Anuncio" table="Anuncio">
    <id name="Id" access="property" column="idAnuncio">
      <generator class="native" />
    </id>
    <property name="Titulo" column="titulo" />
    <property name="Descripcion" column="descripcion" />
    <property name="FechaPublicacion" column="fechaPublicacion" />
    <property name="Estado" column="estado" />
    <many-to-one name="Empresa" column="idEmpresa" />
  </class>
</hibernate-mapping>