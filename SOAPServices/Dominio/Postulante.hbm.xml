<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
  <class name="postulante" table="postulante">
    <id name="Id" access="property" column="idPostulante">
      <!--<generator class="increment" />-->
      <generator class="native" />
    </id>
    <property name="Nombre" column="nombre" />
    <property name="apellidoPaterno" column="apellidoPaterno" />
    <property name="apellidoMaterno" column="apellidoMaterno" />
    <property name="fechaNacimiento" column="fechaNacimiento" />
    <property name="email" column="email" />
    <property name="clave" column="clave" />
  </class>
</hibernate-mapping>
