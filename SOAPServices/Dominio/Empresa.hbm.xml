<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
  <class name="Empresa" table="Empresa">
    <id name="Id" access="property" column="idRubro">
      <!--<generator class="increment" />-->
      <generator class="native" />
    </id>
    <property name="EmailContacto" column="descripcion" />
    <property name="Clave" column="descripcion" />
    <property name="RazonSocial" column="descripcion" />
    <property name="NumeroRuc" column="descripcion" />
    <many-to-one name="Rubro" column="idRubro" />
  </class>
</hibernate-mapping>
